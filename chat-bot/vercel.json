{
  "version": 2,
  "builds": [
    {
      "src": "dist/chat-bot/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/chat-bot/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "dist/chat-bot/browser/assets/$1"
    },
    {
      "src": "/(.*).css",
      "dest": "dist/chat-bot/browser/$1.css"
    },
    {
      "src": "/(.*).js",
      "dest": "dist/chat-bot/browser/$1.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "dist/chat-bot/browser/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "dist/chat-bot/server/server.mjs"
    }
  ]
}
